	<queryString>
		<![CDATA[select a.appName, a.address1, a.address2, c.countryName, p.prodAppNo, prod.prod_name, prod.gen_name, d.dosageform, p.id,
(select message_value from resource_message
    where resource_message.key_bundle =1 and message_key = (concat('ProdAppType.',p.prodAppType))) as prodapptype,
concat(prod.dosage_strength, ' ', u.uom) as dosstrength, p.submitDate,
  group_concat(distinct concat (comp.companyName) separator ', ') as manuf

from `ProdApplications` p, product prod, applicant a, country c, dosform d, `DosUom` u, company comp,prod_company pc
where prod.id = p.prod_id
and a.`applcntId` = p.app_id
and a.`CNTRY_ID` = c.id
and d.uid = prod.`DOSFORM_ID`
and u.id = prod.`DOSUNIT_ID`
and comp.id = pc.company_id
and pc.companyType = 'FIN_PROD_MANUF'
and pc.prod_id = p.PROD_ID
and p.id = $P{id}]]>
	</queryString>
