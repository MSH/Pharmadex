	<queryString>
		<![CDATA[select a.appName, concat(a.address1,', ', a.address2,', ', c.countryName) as address, p.prodAppNo, prod.prod_name, prod.gen_name,
d.dosageform, prod.id, p.prodAppType, concat(prod.dosage_strength, ' ', u.uom) as dosstrength, p.submitDate,
  group_concat(distinct concat (i.name) SEPARATOR '; ') as inn,
  group_concat(distinct concat (e.name) SEPARATOR '; ') as excipient,
  group_concat(distinct concat (comp.companyName) separator ', ') as manuf,
  case WHEN prod.new_chemical_entity THEN 'Substância Activa Conhecida'
    ELSE 'Substância Activa Nova'
  END as nce,
  prod.prod_desc, prod.shelfLife, prod.fnm, prod.drugType, p.registrationDate, p.regExpiryDate
from product prod,`ProdApplications` p,  applicant a, country c, dosform d, `DosUom` u,
company comp,prod_company pc, inn i, prodinn pi, excipient e, prodexcipient pe
where prod.id = p.prod_id
and a.`applcntId` = p.app_id
and a.`CNTRY_ID` = c.id
and d.uid = prod.`DOSFORM_ID`
and u.id = prod.`DOSUNIT_ID`
and comp.id = pc.company_id
and pc.companyType = 'FIN_PROD_MANUF'
and pc.prod_id = p.PROD_ID
and i.id = pi.INN_ID
and pi.prod_id = p.PROD_ID
and e.id = pe.EXPNT_ID
and pe.prod_id = p.PROD_ID
and p.id = $P{id}
group by p.id]]>
	</queryString>
